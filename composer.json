{
    "name": "devsfort/laravel-pigeon-chat",
    "description": "A Laravel package that allows you to add a complete user messaging system into your new/existing Laravel application with only a few easy steps..",
    "keywords": ["laravel", "nodejs", "socket","chat","express","messaging","broadcasting"],
    "license": "MIT",
    "authors": [
        {
            "name": "Haseeb Ahmad",
            "email": "hahmad748@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.4",
        "laravel/framework": ">=8.0"
    },
    "autoload": {
        "psr-4": {
            "DevsFort\\Pigeon\\Chat\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "DevsFort\\Pigeon\\Chat\\ChatServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
